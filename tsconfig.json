{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "module": "esnext"
  },
  "exclude": ["node_modules", "build", "dist"],
  "types": ["react", "react-native"]
}
